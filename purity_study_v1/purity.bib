issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
keywords = {Chromosomes,DNA,Genetic Variation,High-Throughput Nucleotide Sequencing,Human,Humans,Pair 17,Pair 17: genetics,Sequence Analysis,Software},
mendeley-groups = {contamination\_metagenomic\_analysis,DHS purity report},
month = feb,
number = {4},
pages = {593--4},
pmid = {22199392},
title = {{ART: a next-generation sequencing read simulator.}},
