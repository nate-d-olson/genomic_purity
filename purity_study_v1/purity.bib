journal = {Journal of computational biology : a journal of computational molecular cell biology},
keywords = {benchmarking,burrows-wheeler transform,hashing,ngs,short read alignment,spaced seeds,suffix array,suffix tree},
mendeley-groups = {CCQM References,CCQM,CCQM References/mapping assembly alignment,snp review},
